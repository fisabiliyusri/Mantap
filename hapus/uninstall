systemctl stop stunnel5
pkill stunnel4
pkill stunnel5
pkill squid
pkill squid3
pkill squid-1
pkill squid-3
pkill python
cd
rm -rf /etc/systemd/system/ws-nontls.service
rm -rf /etc/systemd/system/ws-tls.service
rm -rf /usr/local/bin/ws-nontls
rm -rf /usr/local/bin/ws-tls
rm -rf /etc/systemd/system/ws-ovpn.service
rm -rf /usr/local/bin/ws-ovpn
rm -rf /etc/default/sslh
rm -rf -f stunnel
rm -rf stunnel5.zip
rm -rf /etc/stunnel5/stunnel5.conf
